{
  "name": "@otezz/heroicons-vue",
  "version": "0.2.1",
  "author": "Seto Kuslaksono <kuslaksono@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node buildIcons && vue-cli-service build --target lib --name heroicons-vue src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:icons": "node buildIcons"
  },
  "main": "./dist/heroicons-vue.common.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "heroicons": "^0.4.2",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/otezz/heroicons-vue/issues"
  },
  "homepage": "https://github.com/otezz/heroicons-vue",
  "keywords": [
    "heroicons",
    "vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/otezz/heroicons-vue.git"
  }
}
